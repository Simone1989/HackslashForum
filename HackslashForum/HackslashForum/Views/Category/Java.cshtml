@model IEnumerable<HackslashForum.Post>



<div class="content">

@foreach (var item in Model)
{
    int cid = item.Id;

    <div id="outerpost">
        <div class="post">
            <a href="https://localhost:44369/Posts/Post/@cid">@item.Title</a>
        
            <p> @Html.DisplayFor(modelItem => item.DownVotes)</p><img src="~/images/Downvote.png" style="float:right;" width="20" height="20" />
            <img src="~/images/Upvote.png" style="float:right;" width="20" height="20" /><p> @Html.DisplayFor(modelItem => item.UpVotes)</p>
        </div>
    </div>
}

</div>